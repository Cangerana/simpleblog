<p style="color: green"><%= notice %></p>

<h1>Tags:1</h1>

<div id="tags">
  <ul>
    <% @tags.each do |tag| %>
      <li>
        <p><%= link_to tag.name, edit_tag_path(tag) %></p>
      </li>
    <% end %>
  </ul>
</div>

<div>
  <button>
  <%= link_to "New tag", new_tag_path %>
  </button>
</div>
